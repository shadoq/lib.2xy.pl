{class:mobi.shad.s3lib.gfx.g3d.shaders.SimpleShader$ShaderData,name:glsl_20399.2,type:null,id:null,date:null,author:null,description:null,tags:null,color:false,normal:false,textures:0,vertex:"void main(){\n\tgl_Position = projectionMatrix * modelViewMatrix * a_position;\n}",fragment:"void main( void ) {\n\n    vec2 p = floor(gl_FragCoord.xy / 20.0);\n    float t1 = mod( (p.x + p.y) * sin(iTime * 0.6), 2.0);\n    float t2 = mod( p.x + p.y * sin(iTime * 0.9), 2.0);\n    float t3 = mod( ((p.x  * sin(iTime * 0.7)) + p.y), 2.0);\n    gl_FragColor = vec4( t1-t3 + .5 * t2, t3-t2+t1 + t1, t3-t1 * t2 * t2 * 2., 1.0);\n}\n"}