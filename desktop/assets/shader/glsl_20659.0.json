{class:mobi.shad.s3lib.gfx.g3d.shaders.SimpleShader$ShaderData,name:glsl_20659.0,type:null,id:null,date:null,author:null,description:null,tags:null,color:false,normal:false,textures:0,vertex:"void main(){\n\tgl_Position = projectionMatrix * modelViewMatrix * a_position;\n}",fragment:"// By: Brandon Fogerty\n// bfogerty at gmail dot com\n\nvoid main( void ) \n{\n    vec2 p = ( gl_FragCoord.xy / iResolution.xy ) * 2.0 - 1.0;\n    \n    vec3 c = vec3( 0.0 );\n    \n    float amplitude = 0.05; \n    float glowT = sin(iTime) * 0.2 + 0.1;\n    float glowFactor = mix( 0.5, 0.20, glowT );\n    \n    c += vec3(0.02, 0.03, 0.13) * ( glowFactor * abs( 1.0 / sin(p.x + sin( p.y + iTime ) * amplitude ) ));\n    c += vec3(0.02, 0.10, 0.03) * ( glowFactor * abs( 1.0 / sin(p.x + cos( p.y + iTime+1.00 ) * amplitude+0.1 ) ));\n    c += vec3(0.15, 0.05, 0.20) * ( glowFactor * abs( 1.0 / sin(p.y + sin( p.x + iTime+1.30 ) * amplitude+0.15 ) ));\n    c += vec3(0.20, 0.05, 0.05) * ( glowFactor * abs( 1.0 / sin(p.y + cos( p.x + iTime+3.00 ) * amplitude+0.3 ) ));\n    c += vec3(0.17, 0.17, 0.05) * ( glowFactor * abs( 1.0 / sin(p.y + cos( p.x + iTime+5.00 ) * amplitude+0.2 ) ));\n    \n    gl_FragColor = vec4( c, 50.0 );\n\n}"}