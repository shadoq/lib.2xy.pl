{class:mobi.shad.s3lib.gfx.g3d.shaders.SimpleShader$ShaderData,name:glsl_19975.0,type:null,id:null,date:null,author:null,description:null,tags:null,color:false,normal:false,textures:0,vertex:"void main(){\n\tgl_Position = projectionMatrix * modelViewMatrix * a_position;\n}",fragment:"/*\n  Daily an hour GLSL sketch by @chimanaco 3/30\n\n  References:\n  http://tokyodemofest.jp/2014/7lines/index.html\n*/\n\nfloat PI = 3.1415926535;\n\nvoid main( void ) {\n    vec2 p=(gl_FragCoord.xy -.5 * iResolution)/ min(iResolution.x,iResolution.y);\n    vec3 c = vec3(0);\n   \n    for(int i = 0; i < 20; i++){\n    float t = 2.* PI * float(i) / 20. * iTime*0.5;\n    float x = cos(t) * sin(t);\n    float y = sin(t);\n    vec2 o = 0.45 * vec2(x,y);\n    float r = fract(t);\n    float g = 1.-r;\n    //float b = 1.-r;\n    c += 0.005/(length(p-o))*vec3(r,g,1);\n    }\n    gl_FragColor = vec4(c,1);\n}"}