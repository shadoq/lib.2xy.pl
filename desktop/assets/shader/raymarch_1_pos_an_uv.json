{class:mobi.shad.s3lib.gfx.g3d.shaders.SimpleShader$ShaderData,name:raymarch_1,type:null,id:null,date:null,author:null,description:null,tags:null,color:false,normal:false,textures:0,vertex:"void main(){\n\tgl_Position = projectionMatrix * modelViewMatrix * a_position;\n}",fragment:"\n//\n// Simple Posistion Raymatch Demo ...\n//\nvoid main() {\n    vec2 pos=(gl_FragCoord.xy * 2.0 - iResolution.xy) / iResolution.y;\n    vec2 uv=gl_FragCoord.xy/iResolution.xy;\n    vec3 outColor=vec3(0.0, 0.0, 0.0);\n\n    //\n    // pos.xy 0.0 - at Center Screen\n    //\n    if (pos.y>0.0){\n        outColor.y=0.2;\n    } else {\n        outColor.y=0.4;\n    }\n    if (pos.x>0.0){\n        outColor.x=0.2;\n    } else {\n        outColor.x=0.4;\n    }\n\n    //\n    // uv.xy 0.0 - at left bottom conner\n    //\n    outColor.x+=uv.x;\n    outColor.z+=uv.y;\n    \n    gl_FragColor=vec4(outColor.r, outColor.g, outColor.b, 1.0);\n}"}