{class:mobi.shad.s3lib.gfx.g3d.shaders.SimpleShader$ShaderData,name:glsl_20037.0,type:null,id:null,date:null,author:null,description:null,tags:null,color:false,normal:false,textures:0,vertex:"void main(){\n\tgl_Position = projectionMatrix * modelViewMatrix * a_position;\n}",fragment:"#define PI 3.1415926535\n\nvoid main( void ) {\n    vec2 p = (2.0*gl_FragCoord.xy - iResolution.xy)/iResolution.y;\n    //p = floor(p)+fract(p*4.0);\n    vec3 col = vec3(0.2);\n    for(int i=0; i<20; i++) {\n        float a = 2.0*PI*float(i)/12.0 + iTime*0.2;\n        float x = cos(iTime)*cos(a)-sin(a);\n        float y = sin(iTime)*sin(a-iTime)+sin(iTime)*cos(a-iTime);\n        vec2 q = cos(iTime)*vec2(x,y);\n        float r = fract(a);\n        float g = 1.0-r;\n        col += 0.01/length(p-q)*vec3(r, g, 0.01);\n    }\n    gl_FragColor = vec4(vec3(col), 1.0);\n}"}