{class:mobi.shad.s3lib.gfx.g3d.shaders.SimpleShader$ShaderData,name:glsl_20036.0,type:null,id:null,date:null,author:null,description:null,tags:null,color:false,normal:false,textures:0,vertex:"void main(){\n\tgl_Position = projectionMatrix * modelViewMatrix * a_position;\n}",fragment:"#define pi 3.1415926535\n\nfloat hash(float x) {\n    return fract(sin(x)*45768.2345);\n}\n\nvoid main( void ) {\n    vec2 p = (2.0*gl_FragCoord.xy - iResolution.xy)/iResolution.y;\n    vec3 col = vec3(0.0);\n    for(int i = 0; i < 24; i++) {\n        float q = hash(cos(float(i))*sin(float(i)));\n        float t = iTime*q;\n        float a = 2.0*pi*float(i)/12.0 + iTime*0.5;\n        float x = cos(q + t)*sin(t);\n        float y = cos(a)*cos(q*q*q + t);\n        vec2 o = 0.7*vec2(x, y);\n        float l = 0.02/length(p-o);\n        col+= l*vec3(0.2, 0.1, 0.6);\n    }\n    gl_FragColor = vec4(col, 1.0);\n}"}